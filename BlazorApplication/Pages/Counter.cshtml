@page "/counter"
@inherits BlazorApplication.CounterClass

<h1>Counter</h1>
<h2>@SubTitle</h2>

<p>Current count: @CurrentCount</p>

<button class="btn btn-primary" onclick="@IncrementCount">Click me</button>

@*@functions {
    int currentCount = 0;

    void IncrementCount()
    {
        currentCount++;
    }
}*@
